{
  "name": "bookshop.ui",
  "version": "0.1.0",
  "private": true,
  "sapux": true,
  "description": "A Fiori elements application.",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "type": "commonjs",
  "scripts": {
    "start": "npx ui5 serve",
    "lint": "npx eslint webapp",
    "karma": "npx karma start",
    "karma-ci": "npx karma start karma-ci.conf.js",
    "test": "npm run lint && npm run karma-ci",
    "build": "ui5 build --clean-dest",
    "build-self-contained": "npx ui5 build self-contained -a --clean-dest",
    "dist": "npx ws --compress --directory dist --port 8080"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@sap/ux-ui5-tooling": "^1.0.28",
    "@sapui5/ts-types": "^1.84.4",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "@ui5/cli": "^2.6.4",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.2",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-ui5": "^2.3.1",
    "local-web-server": "^4.2.1",
    "typescript": "^4.0.5",
    "ui5-middleware-babel": "^2.1.0",
    "ui5-middleware-http-proxy": "^1.1.1",
    "ui5-middleware-lrep-flex": "^0.1.1",
    "ui5-task-babel": "^2.1.0",
    "ui5-task-copy-shim-resources": "^0.1.0"
  },
  "ui5": {
    "dependencies": [
      "@sap/ux-ui5-tooling",
      "ui5-middleware-babel",
      "ui5-middleware-http-proxy",
      "ui5-middleware-lrep-flex",
      "ui5-task-babel",
      "ui5-task-copy-shim-resources",
      "core-js-bundle",
      "regenerator-runtime"
    ]
  },
  "dependencies": {
    "core-js-bundle": "^3.7.0",
    "regenerator-runtime": "^0.13.7"
  }
}
